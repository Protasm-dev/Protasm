.SECTION STRUCTURE  
    BTA1  # First beta-strand  
    BTA2  # Second beta-strand  
    BTA3  # Third beta-strand  
    BTA4  # Fourth beta-strand  

.SECTION LINKAGES  
    BHP1 (BTA1, BTA2)  # Beta-hairpin links strand 1 and 2  
    BHP2 (BTA3, BTA4)  # Beta-hairpin links strand 3 and 4  
    LOP1 (BTA2, BTA3)  # Loop connects strands 2 and 3  

.SECTION SUPERSECONDARY  
    GRK1 (BTA1, BTA2, BTA3, BTA4)  # Greek Key motif stabilizes the sheet
